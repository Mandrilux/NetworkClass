#!/usr/bin/env python3                                                    
# -*- coding: latin-1 -*-                                                 

try:
    import sys
except:
    print ("Error: Cannot load lib 'sys'", file=sys.stderr)
    sys.exit(84)

def usage():
    print ("USAGE")
    print("\tnetwork [network] [number of subnet] [subnet1] ...")
    sys.exit(84)


if __name__ == '__main__':
    if (len(sys.argv) < 2):
        usage()
